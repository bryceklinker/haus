{
  "name": "haus-acceptance-tests",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Bryce Klinker",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean:haus": "rimraf ./haus-web",
    "prebuild:haus": "yarn clean:haus",
    "build:haus": "dotnet publish ../../src/Haus.Web.Host/Haus.Web.Host.csproj --configuration Release --output ./haus-web",
    "start:haus": "cd ./haus-web && dotnet ./Haus.Web.Host.dll",
    "stop:haus": "kill-port 5001",
    "pretest": "yarn build:haus",
    "test": "concurrently \"yarn start:haus\" \"node test-runner.js && yarn stop:haus || (yarn stop:haus && false)\"",
    "pretest:dev": "yarn build:haus",
    "test:dev": "concurrently \"yarn start:haus\" \"node test-runner.js open && yarn stop:haus || (yarn stop:haus && false)\""
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.0",
    "concurrently": "5.3.0",
    "cross-port-killer": "1.3.0",
    "cypress": "6.3.0",
    "jsonwebtoken": "8.5.1",
    "typescript": "4.1.3",
    "uuid": "8.3.2",
    "wait-on": "5.2.1"
  }
}
